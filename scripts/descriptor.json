
{
    "input": [
        "local:/tmp/file.txt"
    ],

    "container": {
        "workdir": "",
        "name": "busybox",
        "cpu_needed": 1,
        "max_memoryMB": 1024,
        "min_memoryMB": 512,
        "cmd": "/bin/sh -l -c 'cat /input/file.txt > /output/file.txt'"
    },

    "required_outputs": {
        "output_uri": "local:/tmp/test_output/$JOB_ID",
        "file_contents": [{
            "file": "file.txt",
            "to_variable": "a"
        }]
    }
